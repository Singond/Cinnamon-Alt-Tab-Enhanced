{
 "all-workspaces-mode": {
    "type" : "checkbox",
    "default" : false,
    "description" : "Display windows from all workspaces"
 },
 "zoom-on": {
    "type": "checkbox",
    "default": true,
    "dependency" : "all-workspaces-mode",
    "indent": true,
    "tooltip": "Only applicable if there is not enough horizontal space to show all windows at once",
    "description": "Zoom in on the windows of the current workspace"
 },
 "display-origin-arrow": {
    "type" : "checkbox",
    "default" : false,
    "dependency" : "all-workspaces-mode",
    "indent": true,
    "description" : "Display a small arrow, below the icon, to indicate the window you came from"
 },
 "style": {
    "type": "combobox",
    "default": ":system",
    "options" : {
        "Use built-in Alt-Tab's switcher-style setting" : ":system",
        "In-place previews" : "icons+preview",
        "In-place thumbnails" : ":preview-thumbnails",
        "Thumbnails" : "icons+thumbnails",
        "Icons only" : "icons"
    },
    "description": "General switcher style"
 },
 "vertical-alignment": {
    "type" : "combobox",
    "default" : "center",
    "options" : {
        "Top" : "top",
        "Center" : "center",
        "Bottom" : "bottom"
    },
    "description" : "Where to display the window switcher"
 },
 "thumbnails-behind-icons": {
    "type" : "combobox",
    "default" : "never",
    "options" : {
        "Never" : "never",
        "Always" : "always",
        "Behind identical icons" : "behind-identical"
    },
    "description" : "Display a thumbnail behind icons"
 },
 "display-thumbnail-headers": {
    "type" : "checkbox",
    "default" : true,
    "tooltip": "Only applicable to some switcher styles",
    "description" : "Display thumbnail headers, showing the window icon and title"
 },
 "compact-labels": {
    "type": "checkbox",
    "default": true,
    "description": "Display the window title in a single-row label"
 },
 "preferred-monitor": {
    "type": "combobox",
    "default": ":primary",
    "options" : {
        "Primary" : ":primary",
        "Secondary" : ":secondary",
        "Focus monitor" : ":focus",
        "Bottom" : ":bottom"
    },
    "description": "Monitor preference"
 },
 "background-image-enabled": {
    "type": "checkbox",
    "default": "true",
    "description": "Show the desktop background image behind in-place previews and thumbnails"
 },
"background-dim-factor": {
     "type": "scale",
    "default": 0.7,
    "min" : 0.0,
    "max" : 1.0,
    "step" : 0.1,
    "description": "Background dim factor"
 },
 "seppe": {"type": "separator"},
 "urgent-notifications": {
    "type": "checkbox",
    "default": true,
    "description": "Use notifications to bring attention to attention-seeking windows"
 },
 "last-gsettings-switcher-style": {
    "type": "generic",
    "default": ""
 }
}
